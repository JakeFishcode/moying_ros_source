<?xml version="1.0" ?>
<robot name="M300_E5" xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:include filename="$(find moying_mor_description)/urdf/MON300.urdf.xacro" />
  <xacro:include filename="$(find moying_mcr_description)/urdf/accessories/elfin3.urdf.xacro" />

  <xacro:property name="PI" value="3.1415926535897932"/>

    <!-- Extention shell -->
    <joint name="extention_shell_joint" type="fixed">
      <origin
          xyz="0 0 0.39"
          rpy="0 0 0" />
      <parent link="vehicle_body"/>
      <child link="extention_shell"/>
    </joint>
    
    <link name="extention_shell">
      <visual>
        <origin rpy="0 0 ${-pi/2}" xyz="0 0 0 "/>
        <geometry>
          <mesh filename="package://moying_mcr_description/meshes/agv/MOD300_TopCover_Basic.STL"/>
        </geometry>
        <material name="GlassWhite"/>
      </visual>
      <collision>
        <origin rpy="0 0 0" xyz="0 0 0.19"/>
        <geometry>
          <box size="1.05 0.62 0.38"/>
        </geometry>
      </collision>
    </link>
    
    <!-- Body -->
    <joint name="body_base_joint" type="fixed">
      <origin
          xyz="0.27 0 0.76"
          rpy="0 0 0" />
      <parent link="vehicle_body"/>
      <child link="body_base"/>
    </joint>
    
    <link name="body_base">
      <inertial>
        <mass value="2"/>
        <origin rpy="0 0 0" xyz="0 0 0.1 "/>
        <cylinder_inertia_def r="0.05" h="0.2" mass="2"/>
      </inertial>
      <visual>
        <origin rpy="${pi/2} 0 ${-pi/2}" xyz="0 0 0 "/>
        <geometry>
          <mesh filename="package://moying_mcr_description/meshes/Body_base.STL"/>
        </geometry>
        <material name="GlassWhite"/>
      </visual>
      <collision>
        <origin rpy="${pi/2} 0 ${-pi/2}" xyz="0 0 0"/>
        <geometry>
          <mesh filename="package://moying_mcr_description/meshes/Body_base.STL"/>
        </geometry>
      </collision>
    </link>
    
    <joint name="body_joint" type="revolute">
      <origin
          xyz="0 0 0.0605"
          rpy="0 0 0" />
      <parent link="body_base"/>
      <child link="body"/>
      <axis
        xyz="0 0 1" />
      <limit
        lower="-3.16"
        upper="3.16"
        effort="104"
        velocity="1.57" />
    </joint>
    
    <link name="body">
      <inertial>
        <mass value="50"/>
        <origin rpy="0 0 0" xyz="0 0 0.22 "/>
        <cuboid_inertia_def x="0.3" y="0.3" z="0.4" mass="5"/>
      </inertial>
      <visual>
        <origin rpy="0 0 ${pi/2}" xyz="0 0 0 "/>
        <geometry>
          <mesh filename="package://moying_mcr_description/meshes/Body.STL"/>
        </geometry>
        <material name="GlassWhite"/>
      </visual>
      <collision>
        <origin rpy="0 0 ${pi/2}" xyz="0 0 0"/>
        <geometry>
          <mesh filename="package://moying_mcr_description/meshes/Body.STL"/>
        </geometry>
      </collision>
    </link>
    
    <transmission name="body_joint_trans">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="body_joint">
        <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      </joint>
      <actuator name="body_joint_motor">
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
    </transmission>
      
    <!-- Manipulators -->
    <xacro:elfin3_arm prefix="left_arm" base_link="body">
      <origin xyz="0 0.087 0.390" rpy="${-PI/2} ${PI/2} 0" />
    </xacro:elfin3_arm>
    
    <xacro:elfin3_arm prefix="right_arm" base_link="body">
      <origin xyz="0 -0.087 0.390" rpy="${PI/2} ${PI/2} 0" />
    </xacro:elfin3_arm>
    
    <!-- Head -->    
    <joint name="head_joint1" type="revolute">
      <origin
          xyz="0 0 0.541"
          rpy="0 0 0" />
      <parent link="body"/>
      <child link="head_base"/>
      <axis
        xyz="0 0 -1" />
      <limit
        lower="${-pi/3}"
        upper="${pi/3}"
        effort="10"
        velocity="1.57" />
    </joint>
    
    <link name="head_base">
      <inertial>
        <mass value="5"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <cuboid_inertia_def x="0.1" y="0.1" z="0.1" mass="0.5"/>
      </inertial>
      <visual>
        <origin rpy="0 0 ${pi/2}" xyz="0 0 0"/>
        <geometry>
          <mesh filename="package://moying_mcr_description/meshes/humoyoid/YF14_Head_base.stl"/>
        </geometry>
        <material name="GlassWhite"/>
      </visual>
      <collision>
        <origin rpy="0 0 ${pi/2}" xyz="0 0 0"/>
        <geometry>
          <mesh filename="package://moying_mcr_description/meshes/humoyoid/YF14_Head_base.stl"/>
        </geometry>
      </collision>
    </link>
    
    <joint name="head_joint2" type="revolute">
      <origin
          xyz="0 0.022 0"
          rpy="0 0 0" />
      <parent link="head_base"/>
      <child link="head"/>
      <axis
        xyz="0 -1 0" />
      <limit
        lower="${-pi*7/36}"
        upper="${pi*7/36}"
        effort="10"
        velocity="1.57" />
    </joint>
    
    <link name="head">
      <inertial>
        <mass value="5"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <cuboid_inertia_def x="0.1" y="0.1" z="0.1" mass="0.5"/>
      </inertial>
      <visual>
        <origin rpy="${pi/2} 0 ${pi}" xyz="0 0 0"/>
        <geometry>
          <mesh filename="package://moying_mcr_description/meshes/humoyoid/YF14_Head.stl"/>
        </geometry>
        <material name="GlassWhite"/>
      </visual>
      <collision>
        <origin rpy="${pi/2} 0 ${pi}" xyz="0 0 0"/>
        <geometry>
          <mesh filename="package://moying_mcr_description/meshes/humoyoid/YF14_Head.stl"/>
        </geometry>
      </collision>
    </link>
    
    <transmission name="head_joint1_trans">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="head_joint1">
        <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      </joint>
      <actuator name="head_joint1_motor">
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
    </transmission>
    
    <transmission name="head_joint2_trans">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="head_joint2">
        <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      </joint>
      <actuator name="head_joint2_motor">
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
    </transmission>
    
    <!-- Attach the RealSense D435 -->
    <sensor_d435 parent="head" name="head_camera">
        <origin xyz="0.09244 -0.022 0.11176" rpy="0 ${pi/12} 0"/>
    </sensor_d435>
    
</robot>
