<robot
  name="elfin5" xmlns:xacro="http://ros.org/wiki/xacro">
  
  <xacro:include filename="$(find elfin_description)/urdf/materials.xacro" />
  <!-- <xacro:include filename="$(find elfin_description)/urdf/elfin_robot.gazebo" /> has incluede in M300.xacro--> 
  
  <xacro:arg name="simulation" default="false"/>
  <xacro:property name="use_gazebo" value="$(arg simulation)"/>
  
  <xacro:if value="${use_gazebo}">
    <gazebo reference="elfin_link2">
      <visual name='visual'>
       <material>
         <script>
           <uri>file://media/materials/scripts/gazebo.material</uri>
           <name>Gazebo/Grey</name>
         </script>
       </material>
      </visual>
    </gazebo>
    <gazebo reference="elfin_link4">
      <visual name='visual'>
       <material>
         <script>
           <uri>file://media/materials/scripts/gazebo.material</uri>
           <name>Gazebo/Grey</name>
         </script>
       </material>
      </visual>
    </gazebo>
  </xacro:if>
  
  <xacro:include filename="$(find elfin_description)/urdf/elfin_transmission.xacro" />
  
  <xacro:property name="PI" value="3.1415926535897931"/>
  
  <xacro:property name="density" value="3.09"/>
  
  <link name="elfin_base_link"/>
  
  <joint name="elfin_base_joint" type="fixed">
    <origin
      xyz="0 0 0"
      rpy="0 0 ${PI/2}" />
    <parent
      link="elfin_base_link" />
    <child
      link="elfin_base" />
  </joint>
  
  <link
    name="elfin_base">
    <inertial>
      <origin
        xyz="0.000209 0.001319 0.025619"
        rpy="0 0 0" />
      <mass value="${0.690521*density}"/>
      <inertia
      ixx="${0.00130611*density}" ixy="${0.0000568*density}" ixz="${0.00000189*density}"
      iyy="${0.00134467*density}" iyz="${0.00001095*density}"
      izz="${0.00229139*density}"/>
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://elfin_description/meshes/elfin5/elfin_base.STL" />
      </geometry>
      <material
        name="metal_white"/>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://elfin_description/meshes/elfin5/elfin_base.STL" />
      </geometry>
    </collision>
  </link>
  <link
    name="elfin_link1">
    <inertial>
      <origin
        xyz="-0.0217491 0.1389011 0.0000125"
        rpy="0 0 0" />
      <mass value="${2.576772*density}"/>
      <inertia
      ixx="${0.01298966*density}" ixy="${0.00287966*density}" ixz="${-0.000001*density}"
      iyy="${0.00721115*density}" iyz="${-0.00000052*density}"
      izz="${0.01532713*density}"/>
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://elfin_description/meshes/elfin5/elfin_link1.STL" />
      </geometry>
      <material
        name="metal"/>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://elfin_description/meshes/elfin5/elfin_link1.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="elfin_joint1"
    type="revolute">
    <origin
      xyz="0 0 0.03"
      rpy="${PI/2} 0 0" />
    <parent
      link="elfin_base" />
    <child
      link="elfin_link1" />
    <axis
      xyz="0 1 0" />
    <limit
      lower="${-2*PI}"
      upper="${2*PI}"
      effort="200"
      velocity="3.14" />
  </joint>
  <link
    name="elfin_link2">
    <inertial>
      <origin
        xyz="-0.0522485 0.1580101 -0.0000003"
        rpy="0 0 0" />
      <mass value="${1.208607*density}"/>
      <inertia
      ixx="${0.02975970*density}" ixy="${-0.00126441*density}" ixz="${-0.00000024*density}"
      iyy="${0.00168156*density}" iyz="${-0.0000021*density}"
      izz="${0.02944694*density}"/>
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://elfin_description/meshes/elfin5/elfin_link2.STL" />
      </geometry>
      <material
        name="metal_grey"/>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://elfin_description/meshes/elfin5/elfin_link2.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="elfin_joint2"
    type="revolute">
    <origin
      xyz="-0.078069 0.19 0"
      rpy="0 0 0" />
    <parent
      link="elfin_link1" />
    <child
      link="elfin_link2" />
    <axis
      xyz="-1 0 0" />
    <limit
      lower="${-0.75*PI}"
      upper="${0.75*PI}"
      effort="200"
      velocity="3.14" />
  </joint>
  <link
    name="elfin_link3">
    <inertial>
      <origin
        xyz="0.0587091 -0.0414608 -0.0000210"
        rpy="0 0 0" />
      <mass value="${1.525883*density}"/>
      <inertia
      ixx="${0.00527835*density}" ixy="${0.00124150*density}" ixz="${-0.00000018*density}"
      iyy="${0.00317861*density}" iyz="${-0.00000067*density}"
      izz="${0.00652527*density}"/>
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://elfin_description/meshes/elfin5/elfin_link3.STL" />
      </geometry>
      <material
        name="metal"/>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://elfin_description/meshes/elfin5/elfin_link3.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="elfin_joint3"
    type="revolute">
    <origin
      xyz="0 0.38 0"
      rpy="${PI} 0 0" />
    <parent
      link="elfin_link2" />
    <child
      link="elfin_link3" />
    <axis
      xyz="1 0 0" />
    <limit
      lower="${-0.85*PI}"
      upper="${0.85*PI}"
      effort="104"
      velocity="3.14" />
  </joint>
  <link
    name="elfin_link4">
    <inertial>
      <origin
        xyz="-0.0587964 0.1436154 -0.0000478"
        rpy="0 0 0" />
      <mass value="${0.475026*density}"/>
      <inertia
      ixx="${0.00542441*density}" ixy="${0.00155092*density}" ixz="${-0.00000053*density}"
      iyy="${0.00131780*density}" iyz="${0.00000195*density}"
      izz="${0.00607489*density}"/>
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://elfin_description/meshes/elfin5/elfin_link4.STL" />
      </geometry>
      <material
        name="metal_grey"/>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://elfin_description/meshes/elfin5/elfin_link4.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="elfin_joint4"
    type="revolute">
    <origin
      xyz="0.078069 -0.11342 0"
      rpy="${-PI} 0 0" />
    <parent
      link="elfin_link3" />
    <child
      link="elfin_link4" />
    <axis
      xyz="0 1 0" />
    <limit
      lower="${-2*PI}"
      upper="${2*PI}"
      effort="104"
      velocity="3.14" />
  </joint>
  <link
    name="elfin_link5">
    <inertial>
      <origin
        xyz="-0.0159154 -0.0365660 0.0000004"
        rpy="0 0 0" />
      <mass value="${0.808399*density}"/>
      <inertia
      ixx="${0.00194399*density}" ixy="${0.00047414*density}" ixz="${0.00000001*density}"
      iyy="${0.00113245*density}" iyz="${-0.00000012*density}"
      izz="${0.00246661*density}"/>
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://elfin_description/meshes/elfin5/elfin_link5.STL" />
      </geometry>
      <material
        name="metal"/>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://elfin_description/meshes/elfin5/elfin_link5.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="elfin_joint5"
    type="revolute">
    <origin
      xyz="0 0.30658 0"
      rpy="${-PI} 0 0" />
    <parent
      link="elfin_link4" />
    <child
      link="elfin_link5" />
    <axis
      xyz="1 0 0" />
    <limit
      lower="${-0.8*PI}"
      upper="${0.8*PI}"
      effort="34"
      velocity="3.14" />
  </joint>
  <link
    name="elfin_link6">
    <inertial>
      <origin
        xyz="-0.0009237 -0.042691 -0.0003367"
        rpy="0 0 0" />
      <mass value="${0.147312*density}"/>
      <inertia
      ixx="${0.00019407*density}" ixy="${1.4e-06*density}" ixz="${-2.89e-06*density}"
      iyy="${0.00019592*density}" iyz="${5e-08*density}"
      izz="${0.00019496*density}"/>
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="${-pi/2} ${pi} 0" />
      <geometry>
        <mesh
          filename="package://moying_mcr_description/meshes/accessories/elfin_end_link.stl" />
      </geometry>
      <material
        name="metal_white"/>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="${-pi/2} ${pi} 0" />
      <geometry>
        <mesh
          filename="package://moying_mcr_description/meshes/accessories/elfin_end_link.stl" />
      </geometry>
    </collision>
  </link>
  <joint
    name="elfin_joint6"
    type="revolute">
    <origin
      xyz="0 -0.1465 0"
      rpy="0 0 0" />
    <parent
      link="elfin_link5" />
    <child
      link="elfin_link6" />
    <axis
      xyz="0 -1 0" />
    <limit
      lower="${-2*PI}"
      upper="${2*PI}"
      effort="34"
      velocity="3.14" />
  </joint>
  
  <link name="elfin_link6_mount"/>
  
  <joint name="elfin_link6_mount_joint" type="fixed">
    <origin
      xyz="0 -0.0085 0"
      rpy="${PI/2} ${PI/2} 0" />
    <parent
      link="elfin_link6" />
    <child
      link="elfin_link6_mount" />
  </joint>
  
  <link name="elfin_end_link"/>
  
  <joint name="elfin_end_joint" type="fixed">
    <origin
      xyz="0 -0.0335 0"
      rpy="${PI/2} ${PI/2} 0" />
    <parent
      link="elfin_link6" />
    <child
      link="elfin_end_link" />
  </joint>
  
  <link name="elfin_dummy_gripper"/>
  
  <joint name="elfin_dummy_joint" type="fixed">
    <origin
      xyz="0 -0.0935 0"
      rpy="${PI/2} ${PI/2} 0" />
    <parent
      link="elfin_link6" />
    <child
      link="elfin_dummy_gripper" />
  </joint>
  
</robot>
